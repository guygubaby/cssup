{
  "name": "@bryce-loskie/cssup",
  "version": "0.0.2",
  "packageManager": "pnpm@7.26.3",
  "description": "Bundle CSS using lightningcss",
  "author": "guygubaby <guygubaby@gmail.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/guygubaby",
  "homepage": "https://github.com/guygubaby/cssup#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guygubaby/cssup.git"
  },
  "bugs": {
    "url": "https://github.com/guygubaby/cssup/issues"
  },
  "keywords": [
    "css",
    "lightningcss",
    "bundler"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "bin": {
    "cssup": "./bin/cssup.mjs"
  },
  "files": [
    "dist",
    "bin"
  ],
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "prepublishOnly": "pnpm build",
    "start": "tsx src/index.ts",
    "dev": "pnpm build --watch",
    "build": "tsup",
    "release": "bumpp --commit --push --tag",
    "lint": "eslint .",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "lightningcss": "^1.18.0"
  },
  "devDependencies": {
    "@bryce-loskie/eslint-config": "^2.0.12",
    "@bryce-loskie/tc": "^0.1.5",
    "@types/node": "^18.11.18",
    "bumpp": "^8.2.1",
    "cac": "^6.7.14",
    "chokidar": "^3.5.3",
    "eslint": "^8.33.0",
    "picocolors": "^1.0.0",
    "tsup": "^6.5.0",
    "tsx": "^3.12.2",
    "typescript": "^4.9.5",
    "vitest": "^0.28.4"
  }
}
