{
  "name": "@bryce-loskie/cssup",
  "version": "0.1.0",
  "packageManager": "pnpm@8.7.1",
  "description": "Bundle CSS using lightningcss",
  "author": "guygubaby <guygubaby@gmail.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/guygubaby",
  "homepage": "https://github.com/guygubaby/cssup#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guygubaby/cssup.git"
  },
  "bugs": {
    "url": "https://github.com/guygubaby/cssup/issues"
  },
  "keywords": [
    "css",
    "lightningcss",
    "bundler"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "bin": {
    "cssup": "./bin/cssup.mjs"
  },
  "files": [
    "dist",
    "bin"
  ],
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "prepublishOnly": "pnpm build",
    "start": "tsx src/index.ts",
    "dev": "pnpm build --watch",
    "build": "tsup",
    "release": "bumpp --commit --push --tag && pnpm publish --access public",
    "lint": "eslint .",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "lightningcss": "^1.21.7",
    "unconfig": "^0.3.10"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.41.0",
    "@bryce-loskie/tc": "^0.1.5",
    "@types/node": "^20.5.9",
    "bumpp": "^9.2.0",
    "cac": "^6.7.14",
    "colorette": "^2.0.20",
    "eslint": "^8.48.0",
    "tsup": "^7.2.0",
    "tsx": "^3.12.8",
    "typescript": "^5.2.2",
    "vitest": "^0.34.3"
  }
}
